    "        else:\n",
    "            parent = parent.getParent()\n",
    "    return False\n",
    "\n",
    "\n",
    "def getParentGraph(elem):\n",
    "    parent = elem.getParent()\n",
    "    while parent:\n",
    "        if parent.isA(mx.NodeGraph):\n",
    "            return parent\n",
    "        else:\n",
    "            parent = parent.getParent()\n",
    "    return None\n",
