    "│   └───translation\n",
    "├───lights\n",
    "│   ├───genglsl\n",
    "│   └───genmsl\n",
    "├───pbrlib\n",
    "│   ├───genglsl <-- e.g. target 'genglsl's implementations reside here\n",
    "│   │   └───lib\n",
    "│   ├───genmdl\n",
    "│   ├───genmsl\n",
    "│   └───genosl\n",
    "│       ├───legacy\n",
