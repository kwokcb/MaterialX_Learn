   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getDownstreamPorts(nodeName):\n",
    "    downstreamPorts = []\n",
    "    for port in doc.getMatchingPorts(nodeName):\n",
    "        #print('- check port:', port)\n",
    "        #print('- Compare: ', port.getConnectedNode().getName(), ' vs ', nodeName)\n",
    "        #if port.getConnectedNode().getName() == nodeName:\n",
    "        downstreamPorts.append(port)\n",
    "    return downstreamPorts"
   ]
